<mvc:View 
	controllerName="com.rizing.demo.controller.Detail" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns:core="sap.ui.core" 
	xmlns="sap.m">
	<Page id="detailPage" title="{i18n>detailTitle}">
		<content>
			<ObjectHeader
					id="objectHeader"
					title="{SoId}"
					number="{GrossAmount}"
					numberUnit="{CurrencyCode}" >
			</ObjectHeader>
		</content>
	</Page>
</mvc:View>